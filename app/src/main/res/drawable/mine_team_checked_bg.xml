<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">

            <corners android:radius="@dimen/x5" />

            <solid android:color="@color/white" />

            <stroke
                android:width="@dimen/x1"
                android:color="#FF4200" />

        </shape>
    </item>

    <item
        android:width="20dp"
        android:left="45dp"
        android:right="1dp"
        android:bottom="1dp"
        android:top="10dp">
        <bitmap
            android:src="@drawable/mine_rb_icon" />
    </item>
</layer-list>
